{
  "name": "Youtube Ads Finder",
  "version": "1.0",
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": [
        "https://*.youtube.com/watch?*",
        "https://youtube.com/watch?*"
      ],
      "js": ["content.js"]
    }
  ],
  "browser_action": {
    "default_title": "Youtube Ads Finder"
  },
  "background": {
    "scripts": ["background.js"]
  },
  "permissions": ["tabs"]
}